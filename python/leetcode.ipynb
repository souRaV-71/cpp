   "source": [
    "# Longest Common Prefix\n",
    "\n",
    "def longestCommonPrefix(self, strs: list(str)) -> str:\n",
    "    res = \"\"\n",
    "\n",
    "    for i in range(len(str[0])):\n",
    "        for s in strs:\n",
    "            if i == len(s) or s[i] != str[0][i]:\n",
    "                return res\n",
    "        res += str[0][i]\n",
    "\n",
    "    return res"
   ]
